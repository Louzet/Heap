{
    "name": "louzet/heap",
    "description": "",
    "keywords": [
        "louzet",
        "heap"
    ],
    "homepage": "https://github.com/louzet/heap",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mickael Louzet",
            "email": "micklouzet@hotmail.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Louzet\\Heap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Louzet\\Heap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
